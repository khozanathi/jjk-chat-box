# ========================
# Node.js .gitignore file
# ========================

# Ignore node modules (never push this)
node_modules/

# Dependency directories
jspm_packages/

# Optional npm cache directory
.npm/

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# dotenv environment files (optional: keep if needed)
.env
.env.test
.env.production
.env.local

# Build output (if using bundlers)
dist/
build/
tmp/
temp/

# OS-specific junk
.DS_Store
Thumbs.db

# IDEs and editors
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Test coverage output
coverage/

# TypeScript
*.tsbuildinfo

# Next.js
.next/

# Nuxt.js
.nuxt/

# Svelte
.svelte-kit/

# Others
.cache/
.parcel-cache/

# Firebase
.firebase/

# Misc
*.tgz